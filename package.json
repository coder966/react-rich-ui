{
  "name": "react-rich-ui",
  "version": "1.5.3",
  "description": "A set of commonly-used React components with super friendly API",
  "keywords": [
    "React",
    "React Component",
    "form",
    "table",
    "button",
    "confirmation",
    "dialogue",
    "date picker",
    "time picker",
    "from control",
    "steps wizard",
    "wizard"
  ],
  "author": "Khalid H. Alharisi",
  "license": "Apache-2.0",
  "homepage": "https://react-rich-ui.coder966.net/",
  "repository": {
    "type": "git",
    "url": "https://github.com/coder966/react-rich-ui"
  },
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "react-bootstrap": "1.6.1",
    "react-hook-form": "^4.10.2",
    "react-paginate": "7.1.3",
    "react-select": "4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublish": "yarn run build",
    "start": "babel src --out-dir dist --copy-files --watch",
    "start:example": "yarn --cwd ./example run start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
